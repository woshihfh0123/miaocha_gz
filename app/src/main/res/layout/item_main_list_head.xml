<?xml version="1.0" encoding="utf-8"?>
<com.mc.phonelive.custom.MyFrameLayout2 xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:focusable="true"
    android:focusableInTouchMode="true"
    app:mfl_offestY="-55dp"
    app:mfl_ratio="0.973">

    <com.mc.phonelive.custom.RatioImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        android:src="@mipmap/bg_list_gxb"
        android:translationY="-75dp"
        app:ri_ratio="0.973" />

    <com.mc.phonelive.custom.MyRelativeLayout2
        android:id="@+id/item_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_gravity="center"
        android:translationY="-50dp"
        app:mrl_ratio="1.64"
        app:mrl_scaleX="0.35">

        <RelativeLayout
            android:id="@+id/data_group_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true">

            <RelativeLayout
                android:id="@+id/layout1"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:gravity="center">

                <ImageView
                    android:id="@+id/wrap_1"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/icon_main_list_head_1" />

                <com.makeramen.roundedimageview.RoundedImageView
                    android:id="@+id/avatar_1"
                    android:layout_width="54dp"
                    android:layout_height="54dp"
                    android:layout_alignBottom="@id/wrap_1"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="17.5dp"
                    android:scaleType="centerCrop"
                    app:riv_oval="true" />
            </RelativeLayout>

            <TextView android:layout_marginTop="-10dp"
                android:id="@+id/name_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/layout1"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:singleLine="true"
                android:textColor="@color/white"
                android:textSize="15sp" />

            <LinearLayout
                android:id="@+id/group_1"
                android:layout_width="wrap_content"
                android:layout_height="15dp"
                android:layout_below="@id/name_1"
                android:layout_centerHorizontal="true">

                <ImageView
                    android:id="@+id/sex_1"
                    android:layout_width="15dp"
                    android:layout_height="match_parent" />

                <ImageView
                    android:id="@+id/level_1"
                    android:layout_width="30dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp" />
            </LinearLayout>

            <TextView
                android:id="@+id/votes_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/group_1"
                android:layout_centerHorizontal="true"
                android:textColor="@color/gray2"
                android:textSize="12sp" />

            <com.mc.phonelive.custom.MyRadioButton
                android:id="@+id/btn_follow_1"
                android:layout_width="44dp"
                android:layout_height="20dp"
                android:layout_below="@id/votes_1"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="4dp"
                android:background="@drawable/bg_btn_follow"
                android:button="@null" android:visibility="gone"
                android:gravity="center"
                android:textColor="#f9f9f9"
                android:textSize="11sp" />
        </RelativeLayout>


        <ImageView
            android:layout_width="20dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="3dp"
            android:adjustViewBounds="true"
            android:src="@mipmap/icon_main_list_medal_1"
            android:visibility="gone" />

    </com.mc.phonelive.custom.MyRelativeLayout2>

    <com.mc.phonelive.custom.MyRelativeLayout2
        android:id="@+id/item_2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="15dp"
        android:translationY="-20dp"
        app:mrl_ratio="1.90"
        app:mrl_scaleX="0.27">

        <RelativeLayout
            android:id="@+id/data_group_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true">

            <ImageView
                android:id="@+id/wrap_2"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5dp"
                android:src="@mipmap/icon_main_list_head_2" />

            <com.makeramen.roundedimageview.RoundedImageView
                android:id="@+id/avatar_2"
                android:layout_width="53dp"
                android:layout_height="53dp"
                android:layout_alignBottom="@id/wrap_2"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="19dp"
                android:scaleType="centerCrop"
                app:riv_oval="true" />

            <TextView
                android:id="@+id/name_2"  android:layout_marginTop="-10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/wrap_2"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:singleLine="true"
                android:textColor="@color/white"
                android:textSize="15sp" />

            <LinearLayout
                android:id="@+id/group_2"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_below="@id/name_2"
                android:layout_centerHorizontal="true">

                <ImageView
                    android:id="@+id/sex_2"
                    android:layout_width="15dp"
                    android:layout_height="match_parent" />

                <ImageView
                    android:id="@+id/level_2"
                    android:layout_width="30dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp" />
            </LinearLayout>

            <TextView
                android:id="@+id/votes_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/group_2"
                android:layout_centerHorizontal="true"
                android:textColor="@color/gray2"
                android:textSize="12sp" />

            <com.mc.phonelive.custom.MyRadioButton
                android:id="@+id/btn_follow_2"
                android:layout_width="44dp"
                android:layout_height="20dp"
                android:layout_below="@id/votes_2"
                android:layout_centerHorizontal="true"
                android:background="@drawable/bg_btn_follow"
                android:button="@null"
                android:gravity="center" android:visibility="gone"
                android:text="" android:layout_marginTop="4dp"
                android:textColor="#f9f9f9"
                android:textSize="11sp" />
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/no_data_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="invisible">

            <ImageView
                android:layout_width="54dp"
                android:layout_height="54dp"
                android:src="@mipmap/icon_main_list_no_data" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="@string/main_list_no_data_2"
                android:textColor="@color/gray2"
                android:textSize="14sp" />


        </LinearLayout>

        <ImageView
            android:layout_width="20dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="3dp"
            android:adjustViewBounds="true"
            android:src="@mipmap/icon_main_list_medal_2"
            android:visibility="gone" />
    </com.mc.phonelive.custom.MyRelativeLayout2>

    <com.mc.phonelive.custom.MyRelativeLayout2
        android:id="@+id/item_3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_gravity="center_vertical|right"
        android:layout_marginRight="15dp"
        android:translationY="-20dp"
        app:mrl_ratio="1.90"
        app:mrl_scaleX="0.27">

        <RelativeLayout
            android:id="@+id/data_group_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true">

            <ImageView
                android:id="@+id/wrap_3"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5dp"
                android:src="@mipmap/icon_main_list_head_3" />

            <com.makeramen.roundedimageview.RoundedImageView
                android:id="@+id/avatar_3"
                android:layout_width="52dp"
                android:layout_height="52dp"
                android:layout_alignBottom="@id/wrap_3"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="17.0dp"
                android:scaleType="centerCrop"
                app:riv_oval="true" />

            <TextView
                android:id="@+id/name_3"  android:layout_marginTop="-10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/wrap_3"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:singleLine="true"
                android:textColor="@color/white"
                android:textSize="15sp" />

            <LinearLayout
                android:id="@+id/group_3"
                android:layout_width="wrap_content"
                android:layout_height="15dp"
                android:layout_below="@id/name_3"
                android:layout_centerHorizontal="true">

                <ImageView
                    android:id="@+id/sex_3"
                    android:layout_width="15dp"
                    android:layout_height="match_parent" />

                <ImageView
                    android:id="@+id/level_3"
                    android:layout_width="30dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp" />

            </LinearLayout>

            <TextView
                android:id="@+id/votes_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/group_3"
                android:layout_centerHorizontal="true"
                android:textColor="@color/gray2"
                android:textSize="12sp" />

            <com.mc.phonelive.custom.MyRadioButton
                android:id="@+id/btn_follow_3"
                android:layout_width="44dp"
                android:layout_height="20dp"
                android:layout_below="@id/votes_3"
                android:layout_centerHorizontal="true" android:visibility="gone"
                android:background="@drawable/bg_btn_follow"
                android:button="@null" android:layout_marginTop="4dp"
                android:gravity="center"
                android:textColor="#f9f9f9"
                android:textSize="11sp" />
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/no_data_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="invisible">

            <ImageView
                android:layout_width="54dp"
                android:layout_height="54dp"
                android:src="@mipmap/icon_main_list_no_data" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="@string/main_list_no_data_2"
                android:textColor="@color/gray2"
                android:textSize="14sp" />


        </LinearLayout>

        <ImageView
            android:layout_width="20dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="3dp"
            android:adjustViewBounds="true"
            android:src="@mipmap/icon_main_list_medal_3"
            android:visibility="gone" />

    </com.mc.phonelive.custom.MyRelativeLayout2>
</com.mc.phonelive.custom.MyFrameLayout2>